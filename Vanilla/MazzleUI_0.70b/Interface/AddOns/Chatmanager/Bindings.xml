<Bindings>
	<Binding name="CHATMANAGER_PANEL" header="CHATMANAGER_TITLE">
		if (ChatmanagerFrame:IsVisible()) then
			HideUIPanel(ChatmanagerFrame);
		else
			ShowUIPanel(ChatmanagerFrame);
		end
	</Binding>
	<Binding name="CHATMANAGER_SAY">
		ChatMenu_SetChatType(ChatFrame1, "SAY");
	</Binding>
	<Binding name="CHATMANAGER_PARTY">
